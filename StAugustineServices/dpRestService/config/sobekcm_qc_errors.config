<?xml version="1.0" encoding="utf-8"?>
<SobekCM_Config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://sobekrepository.org/schemas/sobekcm_qc_errors_config"
	xsi:schemaLocation="http://sobekrepository.org/schemas/sobekcm_qc_errors_config 
